{% extends 'admin/adminbase.html.twig' %}
{% block body %}
    <a href="{{ path('app_logout') }}">Logout</a>
    <h1>
        {{ title }}
    </h1>
    <a href="">Создать пользователя</a><br>
    <h2>Список пользователей</h2>
    <ul>
        {% for user in users %}
            <li>{{ user.email }}, {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>

{% endblock %}